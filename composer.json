{
  "name": "traw/hreflang-news",
  "description": "Extends TYPO3 EXT:seo hreflang functionality for EXT:news records",
  "type": "typo3-cms-extension",
  "keywords": [
    "TYPO3 CMS",
    "Hreflang",
    "Utility"
  ],
  "authors": [
    {
      "name": "Thomas Rawiel",
      "role": "Developer"
    }
  ],
  "license": "GPL-3.0-or-later",
  "autoload": {
    "psr-4": {
      "TRAW\\HreflangNews\\": "Classes/"
    }
  },
  "require": {
    "typo3/cms-core": "^10.4 || ^11.5 || ^12.4",
    "typo3/cms-seo": "^10.4 || ^11.5 || ^12.4",
    "georgringer/news-seo": "^2.0"
  },
  "replace": {
    "typo3-ter/hreflang-news": "self.version"
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "hreflang_news"
    }
  }
}